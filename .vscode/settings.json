{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/node_modules": true,
    "**/.expo": true,
    "**/dist": true,
    "**/build": true,
    "**/*.log": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/.expo": true,
    "**/dist": true,
    "**/build": true,
    "**/*.log": true
  },
  "files.associations": {
    "*.env": "dotenv",
    "*.env.*": "dotenv"
  },
  "files.watcherExclude": {
    "**/node_modules/**": true,
    "**/.expo/**": true,
    "**/dist/**": true,
    "**/build/**": true
  }
}
